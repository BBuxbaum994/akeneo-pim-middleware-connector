  "require": {
    "spryker/kernel": "^3.0.0",
    "spryker-eco/akeneo-pim": "dev-feature/ECO-1078-service-layer",
    "spryker-middleware/process": "dev-dev"
  },
  "repositories": [
    {
      "type": "git",
